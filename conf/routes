# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /name=:id                   controllers.Application.returnName(id: String)
GET     /ticker=:name&year=:year     controllers.Application.returnPrice(name: String, year: String)
POST    /postendpoint               controllers.Application.postendpoint()
POST    /newpostendpoint            controllers.Application.newpostendpoint()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
